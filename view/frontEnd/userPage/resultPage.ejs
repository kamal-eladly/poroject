<%- include("../include/head.ejs") %> <%- include("../include/navebar.ejs") %>
<%- include("../include/notification.ejs") %>
<div class="results">
  <div class="container">
    <h1 class="text-center">نتيجه امتحنات ( <span><%= user.name %></span> )</h1>
    <div class="row">
      <div class="col-md-10 mx-auto">
        <table class="table table-striped table-bordered text-center">
          <tr>
            <td>رقم الاختبار</td>
            <td>اسم الاعاقه</td>
            <td colspan="2">الحاله</td>
          </tr>
          <% AllUserResult.forEach(element => { %>
          <tr>
            <td>الاختبار رقم ( <%= element.ResultTesting.numberOfTest %> )</td>
            <td><%= element.ResultTesting.TestingDisability.name %></td>
            <td class="">
              <span style="padding:8px 15px" class="alert <%= element.success ? "alert-success" : "alert-danger" %> "
                ><%= element.success ? "ناجح" : "غير ناجح" %></span
              >
              <% if (!element.success) { %>
                <a href="/enterExam/<%= element.ResultTesting.id %>" class="btn btn-primary">اعاده الامتحان</a>
                <a href="/yourTraining" class="btn btn-success">التدريب</a>
              <% } %>
            </td>
          </tr>
          <% }) %>
        </table>
      </div>
    </div>
  </div>
</div>
<%- include("../include/footer.ejs") %> <%-
include("../include/footerScript.ejs") %>
<script src="/frontEnd/assets/js/main.js"></script>
